@article{hultNeutrophilDynamicsAffect2021,
  title = {Neutrophil {{Dynamics Affect Mycobacterium}} Tuberculosis {{Granuloma Outcomes}} and {{Dissemination}}},
  author = {Hult, C. and Mattila, J. T. and Gideon, H. P. and Linderman, J. J. and Kirschner, D. E.},
  year = {2021},
  journal = {Frontiers in Immunology},
  volume = {12},
  pages = {712457},
  issn = {1664-3224 (electronic) 1664-3224},
  doi = {10.3389/fimmu.2021.712457},
  abstract = {Neutrophil infiltration into tuberculous granulomas is often associated with higher bacteria loads and severe disease but the basis for this relationship is not well understood. To better elucidate the connection between neutrophils and pathology in primate systems, we paired data from experimental studies with our next generation computational model GranSim to identify neutrophil-related factors, including neutrophil recruitment, lifespan, and intracellular bacteria numbers, that drive granuloma-level outcomes. We predict mechanisms underlying spatial organization of neutrophils within granulomas and identify how neutrophils contribute to granuloma dissemination. We also performed virtual deletion and depletion of neutrophils within granulomas and found that neutrophils play a nuanced role in determining granuloma outcome, promoting uncontrolled bacterial growth in some and working to contain bacterial growth in others. Here, we present three key results: We show that neutrophils can facilitate local dissemination of granulomas and thereby enable the spread of infection. We suggest that neutrophils influence CFU burden during both innate and adaptive immune responses, implying that they may be targets for therapeutic interventions during later stages of infection. Further, through the use of uncertainty and sensitivity analyses, we predict which neutrophil processes drive granuloma severity and structure.\textcopyright{} Copyright \textcopyright{} 2021 Hult, Mattila, Gideon, Linderman and Kirschner.},
  langid = {english},
  keywords = {*granuloma,*molecular dynamics,*Mycobacterium tuberculosis granuloma,adaptive immunity,animal experiment,animal model,animal tissue,article,calgranulin/ec [Endogenous Compound],CD11b antigen/ec [Endogenous Compound],CD3 antigen/ec [Endogenous Compound],CD8+ T lymphocyte,cell survival,clinical outcome,colony forming unit,computer model,controlled study,CXCL9 chemokine/ec [Endogenous Compound],cytokine release,disease severity,flow cytometry,immunohistochemistry,interleukin 10/ec [Endogenous Compound],lymphocytic infiltration,Macaca fascicularis,metastasis,monocyte chemotactic protein 1/ec [Endogenous Compound],neutrophil chemotaxis,neutrophil count,nonhuman,RANTES/ec [Endogenous Compound],sensitivity analysis,transforming growth factor beta/ec [Endogenous Compound],tumor necrosis factor/ec [Endogenous Compound]}
}

