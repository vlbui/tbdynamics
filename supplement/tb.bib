@article{chenLanthanidebasedFretBiosensors2017,
  title = {Lanthanide-Based Fret Biosensors for Time-Gated Imaging and Detection of Protein-Protein Interactions in Live Mammalian Cells},
  author = {Chen, T. and Pham, H. and Miller, L.},
  year = {2017},
  journal = {FASEB Journal},
  series = {Experimental {{Biology}} 2017, {{EB}}. {{Chicago}}, {{IL United States}}.},
  volume = {31},
  number = {1 Supplement 1},
  issn = {1530-6860},
  abstract = {Forster resonance energy transfer (FRET) using fluorescent proteins (FPs) as donors and acceptors is commonly used to microscopically image proteinprotein interactions in living cells. However, overlapping FP spectra lower signalto- noise because the FRET-sensitized acceptor emission signal is convoluted with donor bleedthrough and directly excited acceptor emission, or crosstalk. Quantitative FRET imaging is even more difficult when the local concentrations of donor-labeled and acceptor labeled proteins are substantially different. We used the well-studied interaction between FK binding protein 12 (FKBP12) and the rapamycin binding domain of m-Tor (FRB) as a model system to show that lanthanide-based biosensors can overcome the limitations of conventional FPFRET. The essential features of this approach include the use of luminescent Tb(III) complexes as FRET donors and the elimination of non-specific fluorescence background that comes from time-gated detection of long-lived (\textasciitilde ms) Tb(III) and Tb(III)-to-FP emission signals. We prepared stably transformed NIH 3T3 fibroblast lines that express a single polypeptide consisting of the following elements (N to C): 1) FRB; 2) Escherichia coli dihydrofolate reductase (eDHFR); 3) a linker sequence; 4) GFP; and 5) FKBP12. A Tb(III) complex (TMPLumi4) that selectively binds to eDHFR completes the sensor. The linker comprises alternating repeats of four glutamic acid residues followed by four arginine or lysine residues (ER/K) that form a rigid alpha helix in solution. Stochastic breaks in the helix permit close approach and binding of affinity elements positioned at each end. The effective concentration of each binding element depends on linker length and not solution concentration. Consequently, the fraction of two elements in the closed (high-FRET) state depends on KD and linker length. We tested constructs with ER/K linker lengths of 10, 20 and 30 nm. Following addition of cell permeable TMP-Lumi4 and rapamycin to culture medium, two-color, time-gated images showed significant, \textasciitilde 3-fold increases in acceptor denominated FRET ratios relative to baseline (without rapamycin). The high sensor dynamic range permitted robust, time-gated detection (Z'-factor {$>$} 0.8) of the FKBP12/FRB interaction and its inhibition by ascomycin in cells grown in 96-well plates.},
  langid = {english},
  keywords = {*biosensor,*gated imaging,*lanthanide,*mammal cell,*protein domain,alpha helix,arginine,ascomycin,binding affinity,chemical binding,culture medium,dihydrofolate reductase,effective concentration,endogenous compound,energy transfer,Escherichia coli,fibroblast culture,fluorescence,human,human cell,NIH 3T3 cell line,nonhuman,rapamycin,stochastic model}
}

@article{cherniaevForecastingTrendsTuberculosis2022,
  title = {Forecasting {{Trends}} in the {{Tuberculosis Epidemic Situation}} in the {{Region}} of the {{Russian Federation}} by {{Dynamic Simulation Model}}},
  author = {Cherniaev, I. A. and Chernavin, P. F. and Tsvetkov, A. I. and Chugaev, Y. P. and Cherniaeva, U. I. and Chernavin, N. P.},
  year = {2022},
  journal = {Studies in health technology and informatics},
  volume = {299},
  pages = {235--241},
  issn = {1879-8365 (electronic) 1879-8365},
  doi = {10.3233/SHTI220990},
  abstract = {The spread of a new coronavirus infection in the last two years together with HIV infection preserves and even increases the potential for the spread of tuberculosis in the world. Sverdlovsk oblast (SO) of Russian Federation is the region with high levels of HIV and tuberculosis (TB). The search for new methods of forecasting of the future epidemic situation for tuberculosis has become particularly relevant. The aim was to develop an effective method for predicting the epidemic situation of tuberculosis using an artificial intelligence (AI) method in the format of a dynamic simulation model based on AI technologies. Statistical data was loaded from the state statistical reporting on TB patients for the period 2007-2017. The parameters were controlled through a system of inequalities. The proposed SDM made it possible to identify and reliably calculate trends of TB epidemiological indicators. Comparison of the predicted values made in 2017 with the actual values of 2018-2021 revealed a reliable coincidence of the trend of movement of TB epidemiological indicators in the region, the maximum deviation was no more than 14.82\%. The forecast results obtained with SDM are quite suitable for practical use. Especially, in operational resource planning of measures to counteract the spread of tuberculosis at the regional level.},
  langid = {english},
  keywords = {*epidemic,*Human immunodeficiency virus infection/ep [Epidemiology],*tuberculosis/ep [Epidemiology],artificial intelligence,forecasting,human,Russian Federation}
}

