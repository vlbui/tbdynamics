  title = {Developing a Tuberculosis Transmission Model That Accounts for Changes in Population Health},
  author = {Oxlade, O. and Schwartzman, K. and Benedetti, A. and Pai, M. and Heymann, J. and Menzies, D.},
  year = {2011},
  journal = {Medical decision making : an international journal of the Society for Medical Decision Making},
  volume = {31},
