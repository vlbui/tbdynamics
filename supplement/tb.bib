  author = {Lee, D. J. and Kumarasamy, N. and Resch, S. and Sivaramakrishnan, G. N. and Mayer, K. and Tripathy, S. and Paltiel, A. D. and Freedberg, K. and Reddy, K. P.},
  year = {2018},
