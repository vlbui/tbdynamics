  title = {Integrating Contact Network Structure into Tuberculosis Epidemiology in Meerkats in {{South Africa}}: {{Implications}} for Control},
  author = {Drewe, J. A. and Eames, K. T. D. and Madden, J. R. and Pearce, G. P.},
  year = {2011},
  journal = {Preventive Veterinary Medicine},
  volume = {101},
  number = {1-2},
