@article{khanAspartatebetasemialdeyhydeDehydrogenasePotential2018,
  title = {Aspartate-Beta-Semialdeyhyde Dehydrogenase as a Potential Therapeutic Target of {{Mycobacterium}} Tuberculosis {{H37Rv}}: {{Evidence}} from in Silico Elementary Mode Analysis of Biological Network Model},
  author = {Khan, S. and Somvanshi, P. and Bhardwaj, T. and Mandal, R. K. and Dar, S. A. and Wahid, M. and Jawed, A. and Lohani, M. and Khan, M. and Areeshi, M. Y. and Haque, S.},
  year = {2018},
  journal = {Journal of Cellular Biochemistry},
  volume = {119},
  number = {3},
  pages = {2832--2842},
  issn = {0730-2312 1097-4644},
  doi = {10.1002/jcb.26458},
  abstract = {The emergence of multi-drug resistant strains and co-occurrence of tuberculosis with HIV creates a major burden to the human health globally. Failure of primary antibacterial therapy necessitates the identification of new mycobacterial drugs. In this study, a comprehensive analysis involving bottom-up systems biology approach was applied wherein we have identified potential therapeutic targets of Mycobacterium tuberculosis infections. Our study prioritized M. tuberculosis therapeutic targets (aspartate-beta-semialdeyhde dehydrogenase [ASD], dihydrodipicolinate reductase and diaminopimelate decarboxylase) based on flux and elementary mode analysis using direct mathematical modeling of the relevant metabolic pathways. Molecular docking and simulation studies of the priority target (ie, ASD) revealed the therapeutic potential of the selected natural products (Huperzine A, Rosmarinic acid, and Curcumin) based ASD inhibitors. The study highlights the crucial role of systems biology in conjunction with molecular interaction (docking) for probing novel leads against an increasingly resistant pathogen, M. tuberculousis.Copyright \textcopyright{} 2017 Wiley Periodicals, Inc.},
  langid = {english},
  keywords = {*aspartate beta semialdeyhyde dehydrogenase/ec [Endogenous Compound],*computer model,*Mycobacterium tuberculosis,*oxidoreductase/ec [Endogenous Compound],amino acid metabolism,article,carboxylyase/ec [Endogenous Compound],controlled study,curcumin/an [Drug Analysis],curcumin/dv [Drug Development],curcumin/pd [Pharmacology],diaminopimelate decarboxylase/ec [Endogenous Compound],dihydrodipicolinate reductase/ec [Endogenous Compound],essential gene,huperzine A/an [Drug Analysis],huperzine A/dv [Drug Development],huperzine A/pd [Pharmacology],lysine/ec [Endogenous Compound],molecular docking,molecular dynamics,nonhuman,priority journal,rosmarinic acid/an [Drug Analysis],rosmarinic acid/dv [Drug Development],rosmarinic acid/pd [Pharmacology],systems biology,unclassified drug}
