@article{nelsonModelingMissingCases2020,
  title = {Modeling Missing Cases and Transmission Links in Networks of Extensively Drug-Resistant Tuberculosis in {{KwaZulu-Natal}}, {{South Africa}}},
  author = {Nelson, K. N. and Gandhi, N. R. and Mathema, B. and Lopman, B. A. and Brust, J. C. M. and Auld, S. C. and Ismail, N. and Omar, S. V. and Brown, T. S. and Allana, S. and Campbell, A. and Moodley, P. and Mlisana, K. and Sarita Shah, N. and Jenness, S. M.},
  year = {2020},
  journal = {American Journal of Epidemiology},
  volume = {189},
  number = {7},
  pages = {735--745},
  issn = {0002-9262 1476-6256},
  doi = {10.1093/aje/kwaa028},
  abstract = {Patterns of transmission of drug-resistant tuberculosis (TB) remain poorly understood, despite over half a million incident cases worldwide in 2017. Modeling TB transmission networks can provide insight into drivers of transmission, but incomplete sampling of TB cases can pose challenges for inference from individual epidemiologic and molecular data. We assessed the effect of missing cases on a transmission network inferred from Mycobacterium tuberculosis sequencing data on extensively drug-resistant TB cases in KwaZulu-Natal, South Africa, diagnosed in 2011-2014. We tested scenarios in which cases were missing at random, missing differentially by clinical characteristics, or missing differentially by transmission (i.e., cases with many links were under- or oversampled). Under the assumption that cases were missing randomly, the mean number of transmissions per case in the complete network needed to be larger than 20, far higher than expected, to reproduce the observed network. Instead, the most likely scenario involved undersampling of high-transmitting cases, and models provided evidence for super-spreading. To our knowledge, this is the first analysis to have assessed support for different mechanisms of missingness in a TB transmission study, but our results are subject to the distributional assumptions of the network models we used. Transmission studies should consider the potential biases introduced by incomplete sampling and identify host, pathogen, or environmental factors driving super-spreading.Copyright \textcopyright{} The Author(s) 2020. Published by Oxford University Press on behalf of the Johns Hopkins Bloomberg School of Public Health. All rights reserved.},
  langid = {english},
  keywords = {*bacterial transmission,*extensively drug resistant tuberculosis,*information processing,*missing data,article,Mycobacterium tuberculosis,sampling,South Africa,statistical bias,whole genome sequencing}
}

@article{nelsonSocialMixingClinical2020,
  title = {Social Mixing and Clinical Features Linked with Transmission in a Network of Extensively Drug-Resistant Tuberculosis Cases in {{KwaZulu-Natal}}, {{South Africa}}},
  author = {Nelson, K. N. and Jenness, S. M. and Mathema, B. and Lopman, B. A. and Auld, S. C. and Sarita Shah, N. and Brust, J. C. M. and Ismail, N. and Omar, S. V. and Brown, T. S. and Allana, S. and Campbell, A. and Moodley, P. and Mlisana, K. and Gandhi, N. R.},
  year = {2020},
  journal = {Clinical Infectious Diseases},
  volume = {70},
  number = {11},
  pages = {2396--2402},
  issn = {1058-4838 1537-6591},
  doi = {10.1093/cid/ciz636},
  abstract = {Background. Tuberculosis (TB) is the leading infectious cause of death globally, and drug-resistant TB strains pose a serious threat to controlling the global TB epidemic. The clinical features, locations, and social factors driving transmission in settings with high incidences of drug-resistant TB are poorly understood. Methods. We measured a network of genomic links using Mycobacterium tuberculosis whole-genome sequences. Results. Patients with 2-3 months of cough or who spent time in urban locations were more likely to be linked in the network, while patients with sputum smear-positive disease were less likely to be linked than those with smear-negative disease. Associations persisted using different thresholds to define genomic links and irrespective of assumptions about the direction of transmission. Conclusions. Identifying factors that lead to many transmissions, including contact with urban areas, can suggest settings instrumental in transmission and indicate optimal locations and groups to target with interventions.Copyright \textcopyright{} The Author(s) 2019.},
  langid = {english},
  keywords = {*bacterial transmission,*extensively drug resistant tuberculosis,*social network,adolescent,adult,anti human immunodeficiency virus agent/dt [Drug Therapy],antiretroviral therapy,article,bioinformatics,CD4 lymphocyte count,child,clinical feature,coughing,cross-sectional study,female,hospital admission,human,Human immunodeficiency virus infection/dt [Drug Therapy],incidence,infant,major clinical study,male,medical record review,newborn,priority journal,retrospective study,single nucleotide polymorphism,South Africa,sputum smear,thorax radiography,urban area,virus genome,whole genome sequencing}
}

@article{nguyenDeclineTuberculosisBurden2021,
  title = {Decline of {{Tuberculosis Burden}} in {{Vietnam Measured}} by {{Consecutive National Surveys}}, 2007\textendash 2017},
  author = {Nguyen, Hai Viet and Nguyen, Hoa Binh and Nguyen, Nhung Viet and Cobelens, Frank and Finlay, Alyssa and Dao, Cu Huy and Mirtskhulava, Veriko and Glaziou, Philippe and Pham, Huyen T.T. and {de Haas}, Petra and Tiemersma, Edine},
