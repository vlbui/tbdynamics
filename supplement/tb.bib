@article{telisingheEffectSystematicScreening2021,
  title = {The Effect of Systematic Screening of the General Population on {{TB}} Case Notification Rates},
  author = {Telisinghe, L. and Shaweno, D. and Hayes, R. J. and Dodd, P. J. and Ayles, H. M.},
  year = {2021},
  journal = {International Journal of Tuberculosis and Lung Disease},
  volume = {25},
  number = {12},
  pages = {964--973},
  issn = {1027-3719 1815-7920},
  doi = {10.5588/ijtld.21.0328},
  abstract = {BACKGROUND: Understanding how TB case notification rates (TB-CNR) change with TB screening and their association with underlying TB incidence/prevalence could inform how they are best used to monitor screening impact. METHODS : We undertook a systematic review to identify articles published between 1 January 1980 and 13 April 2020 on TB-CNR trends associated with TB screening in the general-population. Using a simple compartmental TB transmission model, we modelled TB-CNRs, incidence and prevalence dynamics during 5 years of screening. RESULT S : Of 27,282 articles, seven before/after studies were eligible. Two involved population-wide screening, while five used targeted screening. The data suggest screening was associated with initial increases in TBCNRs. Increases were greatest with population-wide screening, where screening identified a large proportion of notified people with TB. Only one study reported on sustained screening; TB-CNR trendswere compatiblewith model simulations. Model simulations always showed a peak in TB-CNRs with screening. Following the peak, TBCNRs declined but were typically sustained above baseline during the intervention. Incidence and prevalence decreased during the intervention; the relative decline in incidence was smaller than the decline in prevalence. CONCLUS IONS : Published data on TB-CNR trends with TB screening are limited. These data are needed to identify generalisable patterns and enable method development for inferring underlying TB incidence/ prevalence from TB-CNR trends.Copyright \textcopyright{} 2021 International Union against Tubercul. and Lung Dis.. All rights reserved.},
  langid = {english},
  keywords = {*mass screening,*tuberculosis/ep [Epidemiology],active case finding,case finding,case notification rate,clinical outcome,disease exacerbation/ep [Epidemiology],disease transmission,enhanced case finding,epidemiological data,human,incidence,mathematical model,passive case finding,prevalence,review,systematic review,trend study,tuberculosis control}
}

