@article{santosQuantificationAnimalTuberculosis2020,
  title = {Quantification of the Animal Tuberculosis Multi- Host Community Offers Insights for Control},
  author = {Santos, N. and Richomme, C. and Nunes, T. and Vicente, J. and Alves, P. C. and {de la Fuente}, J. and {Correia-neves}, M. and Boschiroli, M. L. and Delahay, R. and Gortazar, C.},
  year = {2020},
  journal = {Pathogens},
  volume = {9},
  number = {6},
  pages = {421},
  issn = {2076-0817 (electronic) 2076-0817},
  doi = {10.3390/pathogens9060421},
  abstract = {Animal tuberculosis (TB) is a multi-host zoonotic disease whose prevalence in cattle herds in Europe has been increasing, despite a huge investment in eradication. The composition of the host community is a fundamental driver of pathogen transmission, and yet this has not been formally quantified for animal TB in Europe. We quantified multi-host communities of animal TB, using stochastic models to estimate the number of infected domestic and wild hosts in three regions: officially TB-free Central-Western Europe, and two largely TB-endemic regions, the Iberian Peninsula and Britain and Ireland. We show that the estimated number of infected animals in the three regions was 290,059-1,605,612 and the numbers of infected non-bovine domestic and wild hosts always exceeded those of infected cattle, with ratios ranging from 3.3 (1.3-19.6):1 in Britain and Ireland to 84.3 (20.5-864):1 in the Iberian Peninsula. Our results illustrate for the first time the extent to which animal TB systems in some regions of Europe are dominated by non-bovine domestic and wild species. These findings highlight the need to adapt current strategies for effective future control of the disease.Copyright \textcopyright{} 2020 by the authors. Licensee MDPI, Basel, Switzerland.},
  langid = {english},
  keywords = {*animal disease/dm [Disease Management],*animal disease/ep [Epidemiology],*host bacterium interaction,*stochastic model,*tuberculosis/dm [Disease Management],*tuberculosis/ep [Epidemiology],article,deer,disease transmission,domestic animal,domestic goat,domestic species,endemic disease,European wild boar,fallow deer,Meles,nonhuman,pig,population,population size,prevalence,red deer,reservoir,sensitivity and specificity,sheep,tuberculin test,wild species,wildlife}
