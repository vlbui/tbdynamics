@article{minPOSC146CostEffectivenessAdherence2022,
  title = {{{POSC146 Cost-Effectiveness}} of {{Adherence}} in {{Newly Treated Tuberculosis Patients Using Discrete Event Simulation}}},
  author = {Min, S. and Kwon, S. H. and Nam, J. H.},
  year = {2022},
  journal = {Value in Health},
  series = {{{ISPOR Europe}} 2021. {{Virtual}}, {{Online}}.},
  volume = {25},
  number = {1 Supplement},
  pages = {S116},
  issn = {1098-3015 1524-4733},
  doi = {10.1016/j.jval.2021.11.550},
  abstract = {Objectives: Despite the national efforts to reduce Tuberculosis (TB) patients, the number of patients in South Korea is considered the largest among OECD countries. Poor adherence to anti-tuberculosis treatment is an obstacle to tuberculosis control. Therefore, we evaluate the cost-effectiveness of adherence to TB treatment by comparing adherent patients and non-adherent patients using simulation techniques. Method(s): We targeted newly-treated TB patients. A discrete-event simulation model was built to compare adherent patients and non-adherent patients to TB treatment. We considered treatment completion, loss to follow-up, recurrence, death, and treatment changes from drug-susceptible TB to Multidrug resistance (MDR) TB. Through analyzing claims data from Korean nationwide health insurance from 2007 to 2015, we obtained TB-related costs and distributions of time-to-event such as death, recurrence, or treatment completion. Utilities from the previous study were adapted. Ten thousand patients who started TB treatments in each group were ten times simulated until 100 years old or death. We estimated the quality-adjusted life-years (QALYs) and costs for each group and an incremental cost-effectiveness ratio (ICER) as an index of cost-effectiveness. The model was developed with R software. Result(s): Average TB-related costs per TB patient were US\$ 2,966 and US\$ 5,782 in the adherent and the non-adherent groups. QALYs per patient were 32.7 and 31.3, respectively. When comparing the adherent group with the non-adherent group, ICER was US\$ -1,950 per QALY. Conclusion(s): Adherence to TB treatments is cost-effective with less cost and more QALY gained. Commitment programs to improve patients' adherence can be helpful to manage TB nationwide.Copyright \textcopyright{} 2021},
  langid = {english},
  keywords = {*cost effectiveness analysis,*discrete event simulation,*tuberculosis,aged,conference abstract,controlled study,female,follow up,health insurance,human,major clinical study,male,multidrug resistance,quality adjusted life year,software,very elderly}
}

