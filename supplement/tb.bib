@article{cohenAreSurveyBasedEstimates2008,
  title = {Are {{Survey-Based Estimates}} of the {{Burden}} of {{Drug Resistant TB Too Low}}? {{Insight}} from a {{Simulation Study}}},
  shorttitle = {Are {{Survey-Based Estimates}} of the {{Burden}} of {{Drug Resistant TB Too Low}}?},
  author = {Cohen, Ted and Colijn, Caroline and Finklea, Bryson and Wright, Abigail and Zignol, Matteo and Pym, Alexander and Murray, Megan},
  year = {2008},
  month = jun,
  journal = {PLOS ONE},
  volume = {3},
  number = {6},
  pages = {e2363},
  publisher = {{Public Library of Science}},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0002363},
  urldate = {2022-10-18},
  abstract = {BackgroundThe emergence of tuberculosis resistant to multiple first- and second-line antibiotics poses challenges to a global control strategy that relies on standard drug treatment regimens. Highly drug-resistant strains of Mycobacterium tuberculosis have been implicated in outbreaks and have been found throughout the world; a comprehensive understanding the magnitude of this threat requires an accurate assessment of the worldwide burden of resistance. Unfortunately, in many settings where resistance is emerging, laboratory capacity is limited and estimates of the burden of resistance are obtained by performing drug sensitivity testing on a sample of incident cases rather than through the use of routine surveillance.Methodology/Principal FindingsUsing an individual-based dynamic tuberculosis model to simulate surveillance strategies for drug resistance, we found that current surveys may underestimate the total burden of resistant tuberculosis because cases of acquired resistance are undercounted and resistance among prevalent cases is not assessed. We explored how this bias is affected by the maturity of the epidemic and by the introduction of interventions that target the emergence and spread of resistant tuberculosis.ConclusionsEstimates of drug resistant tuberculosis based on samples of incident cases should be viewed as a lower bound of the total burden of resistance.},
  langid = {english},
  keywords = {Antibiotic resistance,Drug therapy,Extensively drug-resistant tuberculosis,HIV epidemiology,Infectious disease epidemiology,Mycobacterium tuberculosis,Natural history of disease,Tuberculosis},
  file = {C\:\\Users\\vbui0010\\Zotero\\storage\\2WVG2EJT\\Cohen et al. - 2008 - Are Survey-Based Estimates of the Burden of Drug R.pdf;C\:\\Users\\vbui0010\\Zotero\\storage\\JSSHRCV6\\article.html}
}

@article{cohenAreSurveybasedEstimates2008,
  title = {Are Survey-Based Estimates of the Burden of Drug Resistant {{TB}} Too Low? {{Insight}} from a Simulation Study},
  author = {Cohen, T. and Colijn, C. and Finklea, B. and Wright, A. and Zignol, M. and Pym, A. and Murray, M.},
  year = {2008},
  journal = {PLoS ONE},
  volume = {3},
  number = {6},
  pages = {e2363},
  doi = {10.1371/journal.pone.0002363},
  abstract = {Background: The emergence of tuberculosis resistant to multiple first- and second-line antibiotics poses challenges to a global control strategy that relies on standard drug treatment regimens. Highly drug-resistant strains of Mycobacterium tuberculosis have been implicated in outbreaks and have been found throughout the world; a comprehensive understanding the magnitude of this threat require an accurate assessment of the worldwide burden of resistance. Unfortunately, in many settings where resistance is emerging, laboratory capacity is limited and estimates of the burden of resistance are obtained by performing drug sensitivity testing on a sample of incident cases rather than through the use of routine surveillance. Methodology/Principal Findings: Using an individual-based dynamic tuberculosis model to stimulate surveillance strategies for drug resistance, we found that current surveys may understimate the total burden of resistant tuberculosis because cases of acquired resistance are undercounted and resistance among prevalent cases is not assessed. We explored how this bias is affected by the maturity of the epidemic and by the introduction of interventions that target the emergence and spread of resistant tuberculosis. Conclusion(s): Estimates of drug resistant tuberculosis based on samples of incident cases should be viewed as a lower bound of the total burden of resistance. \textcopyright{} 2008 Cohen et al.}
}

