@article{zhangAirflowNanoparticleDeposition2008,
  title = {Airflow and Nanoparticle Deposition in a 16-Generation Tracheobronchial Airway Model},
  author = {Zhang, Z. and Kleinstreuer, C. and Kim, C. S.},
